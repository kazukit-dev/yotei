{
  "name": "yotei",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev:all": "turbo run dev",
    "check:all": "turbo run lint:check format:check typecheck",
    "check:api": "turbo run lint:check format:check typecheck --filter=@yotei/api",
    "check:web": "turbo run lint:check format:check typecheck --filter=@yotei/web",
    "fix:all": "turbo run lint:fix format:fix",
    "fix:api": "turbo run lint:fix format:fix --filter=@yotei/api",
    "fix:web": "turbo run lint:fix format:fix --filter=@yotei/web",
    "test:all": "turbo run test",
    "test:api": "turbo run test --filter=@yotei/api"
  },
  "keywords": [
    "remix",
    "hono",
    "neverthrow"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "lefthook": "^1.11.13",
    "only-allow": "^1.2.1",
    "turbo": "^2.5.4"
  },
  "engines": {
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.11.0",
  "volta": {
    "node": "22.16.0"
  }
}
